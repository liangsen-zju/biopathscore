citHeader("To cite biopathscore in publications use:")

citEntry(entry = "Article",
  title        = "A Novel Unsupervised Algorithm for Biological Process-based Analysis on Cancer",
  author       = personList(as.person("Tianci Song"),as.person("Sha Cao"),as.person("Sheng Tao"),as.person("Sen Liang"),as.person("Wei Du"),as.person("Yanchun Liang")),
  journal      = "Scientific Reports",
  year         = "2017",
  volume       = "xx",
  number       = "xx",
  pages        = "xx",
  url          = "xxxxxxxxxxxx",

  textVersion  =
  paste("Tianci Song(2011).",
        "A Novel Unsupervised Algorithm for Biological Process-based Analysis on Cancer.",
        "Scientific Reports, 2017.",
        "URL http://www.xxxxxxxxxxxxxx.")
)
